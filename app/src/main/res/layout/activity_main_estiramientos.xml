<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#BBB0DA"
    tools:context=".MainActivity_Estiramientos">

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="339dp"
        android:layout_height="654dp"
        android:layout_margin="8dp"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.916"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="104dp"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/pechofondos24"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="@string/pechofondos24"
                android:onClick="Scrollview"
                app:srcCompat="@drawable/pechofondos24" />

            <ImageButton
                android:id="@+id/brazobueno"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="@string/brazobueno"
                android:onClick="Scrollview"
                app:srcCompat="@drawable/brazobueno" />

            <ImageButton
                android:id="@+id/piernafondo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="@string/piernafondo"
                android:onClick="Scrollview"
                app:srcCompat="@drawable/piernafondo" />

            <ImageButton
                android:id="@+id/espaldafondo1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="@string/espaldafondo1"
                android:onClick="Scrollview"
                app:srcCompat="@drawable/espaldafondo1" />
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>